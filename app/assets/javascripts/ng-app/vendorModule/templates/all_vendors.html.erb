<div class="col-md-12" style="padding-left:0px;margin-top:1%;margin-bottom:1%;">
  <back-button data-toggle="tooltip" title="Click this button to go to previous screen"></back-button>
  <button data-toggle="tooltip" title="Click this button to create new Vendor" class="btn btn-default col-md-1" style="margin-left:1%;" ng-click=go('/index/vendor/create') ><i class="fa fa-plus-square"></i></button>
</div>
<div style="border-bottom: 1px solid #EEEEEE;">
  <h3>All Vendors</h3>
</div>
<div class="row" ng-show="allLocations.length != 0">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Vendor Name</th>
        <th>Vendor Company Name</th>
        <th>Mobile Number</th>
        <th>Email Address</th>
      </tr>
      <tr ng-repeat="location in allLocations">
        <td><a href="javascript:void(0)" ng-href='/#/index/vendor/{{location.id}}'>{{location.name}}</a></td>
        <td>{{location.company_name}}</td>
        <td>{{location.mobile_number}}</td>
        <td>{{location.email_id}}</td>
      </tr>
    </table>
  </div>
</div>
