<div class="col-md-12" style="padding-left:0px;margin-top:1%;margin-bottom:1%;">
  <back-button data-toggle="tooltip" title="Click this button to go to previous screen"></back-button>
  <button tooltip="Click this button to create new Customer" class="btn btn-default col-md-1" style="margin-left:1%;" ng-click="go('/index/customers/create')"><i class="fa fa-plus-square"></i></button>
</div>
<br />
<div style="border-bottom: 1px solid #EEEEEE;" ng-show="showList">
  <h3>All Customers</h3>
</div>
<div class="row" ng-show="allMasterProducts.length != 0 && showList">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Customer Id</th>
        <th>Customer Name</th>
        <th>Email</th>
        <th>Mobile Number</th>
        <th>Billing Name</th>
      </tr>
      <tr ng-repeat="master in allMasterProducts">
        <td>{{master.id}}</td>
        <td><a href="javascript:void(0)" ng-click="go('/index/customers/{{master.id}}')">{{master.name}}</a></td>
        <td>{{master.email_id}}</td>
        <td>{{master.mobile_number}}</td>
        <td>{{master.billing_name}}</td>
      </tr>
    </table>
  </div>
</div>
</div>
