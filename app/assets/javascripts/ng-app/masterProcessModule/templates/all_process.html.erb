<div class="col-md-12" style="padding-left:0px;margin-top:1%;margin-bottom:1%;">
  <back-button data-toggle="tooltip" title="Click this button to go to previous screen"></back-button>
  <button data-toggle="tooltip" title="Click this button to create new Process" class="btn btn-default col-md-1" style="margin-left:1%;" ng-click="go('/index/process/create')"><i class="fa fa-plus-square"></i></button>
</div>
<div style="border-bottom: 1px solid #EEEEEE;" ng-show="showList">
  <h3>All Master Processes</h3>
</div>
<div class="row" ng-show="allMasterProducts.length != 0 && showList">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Process Name</th>
        <th>Process Description</th>
        <th></th>
      </tr>
      <tr ng-repeat="master in allMasterProducts">
        <td><a href="javascript:void(0)" ng-href="/#/index/process/{{master.id}}">{{master.name}}</a></td>
        <td>{{master.description}}</td>
        <td><a href="" ng-click="deleteProcess(master.id)" ><span class="glyphicon glyphicon-remove-sign"></span></a></td>
      </tr>
    </table>
  </div>
</div>
